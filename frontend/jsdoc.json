{
    "tags": {
        "allowUnknownTags": ["component"]
    },
    "plugins": ["plugins/markdown", "better-docs/component"],
    "source": {
        "excludePattern": "node_modules"
    },
    "templates": {
        "better-docs": {
            "name": "My React components"
        }
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "readme": "README.md"
    }
}
